def dia_semana(dia):
    dias_da_semana = {
        1: "domingo",
        2: "segunda-feira",
        3: "terÃ§a-feira",
        4: "quarta-feira",
        5: "quinta-feira",
        6: "sexta-feira",
        7: "sÃ¡bado"
    }
    
    if dia < 1 or dia > 7:
        return ""
    
    return dias_da_semana[dia]